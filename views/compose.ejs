<%- include("partials/header") -%>

<h1>Compose </H1>

<form action="/compose" method="POST">
    <div class="form-group">
        <label for="forheading">Title</label>
        <input class="form-control" type="text" name="postTitle" id="">
    </div>

    <div class="form-group">
        <label for="forarticle">Post</label>
        <textarea class="form-control"  type="text" name="postBody" id="" rows="4"> </textarea>
    </div>
      
    <button class="btn btn-lg btn-primary" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer") -%>